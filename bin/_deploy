#!/bin/bash

source _color_logs

echo_info "Parar o mundo"
./_down

echo_info "Construir o mundo"
./_build

echo_info "Subir o banco"
./db_up

echo_info "Migrar o banco..."

echo_info "... acesso"
./acesso_migrate

echo_info "... perfil"
./perfil_migrate

echo_info "... processo seletivo"
./seletivo_migrate

./acesso_run python manage.py init_dev

echo_info "Parar o mundo novamente"
./_down

echo_success "Tudo pronto"
    
